{

  "manifest_version": 2,
  "name": "Pinned Tab Fix",
  "version": "2.2.3",

  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/nralbrecht/pinned-tab-fix",
  "default_locale": "en",

  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },

  "background": {
    "scripts": ["background-script.js"]
  },

  "options_ui": {
      "page": "options.html"
  },

  "permissions": [
    "storage",
    "tabs"
  ]

}
